{
  "manifest_version": 3,
  "name": "Auto Tab Grouper",
  "version": "1.2",
  "description": "Groeperen van tabbladen op basis van base URL.",
  "permissions": [
    "tabs",
    "tabGroups"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
